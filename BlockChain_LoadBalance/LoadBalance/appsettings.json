{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RedisConn": "redis",
  "RedisCacheTimeOutInSecond": "15",

  "NodeCheck": {
    "ErrorThreshold" : 5 
  } ,

  "EthNodeChoose": {
    "Choose": "NodeChooseByBlockHeight"
  }, 

  "Db": {
    "Type": "mysql"
    
  } ,

  "Cache": {
    "Eth_IrrevocableBlockCount": 36



  },

  "Auth": {
    //Auth.Type In Token , Ip , None
    "Type": "None",
    "AllowIp": "127.0.0.1|192.168.1.153"
  } 


}
